{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "Node16",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "dom"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@commands/*": [
        "src/commands/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@webviews/*": [
        "src/webviews/*"
      ]
    },
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
  }
}